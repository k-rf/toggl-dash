// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Toggl Config
// -----------------------------------------------------------------------------

model TogglConfig {
  key   String @id
  value String
}

// Domain
// -----------------------------------------------------------------------------

model DashButton {
  id      String @id @db.Uuid
  summary String
  order   Int
}
