{
  "name": "toggl_dash_backend",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "cross-env NODE_ENV=development dotenv -e .env.development -- nest start",
    "start:dev": "yarn start --watch",
    "start:debug": "yarn start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production dotenv -e .env.production -e.env.production.local -- node dist/main",
    "check:lint": "eslint \"{src,test}/**\" --fix --cache --ext .tsx,.ts,.js",
    "check:format": "prettier . --write",
    "check:type": "tsc --project tsconfig.json --pretty --noEmit",
    "check:all": "npm-run-all -s check:type check:format check:lint",
    "test": "cross-env NODE_ENV=test dotenv -e .env.test -- jest --runInBand",
    "test:cov": "yarn test --coverage=true",
    "test:watch": "yarn test --watch",
    "test:e2e": "cross-env NODE_ENV=test-e2e dotenv -e .env.test -- jest --config ./test/jest-e2e.js --runInBand",
    "codegen": "ts-node src/graphql/generate-typings.ts && yarn prettier ./src/graphql --write"
  },
  "lint-staged": {
    "*.{tsx,ts,js}": [
      "bash -c 'yarn check:type'",
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/serve-static": "^3.0.0",
    "@prisma/client": "^4.8.0",
    "apollo-server": "^3.11.1",
    "apollo-server-express": "^3.11.1",
    "axios": "^1.2.1",
    "cookie-parser": "^1.4.6",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "ts-pattern": "^4.1.3",
    "uuid": "^9.0.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@tsd/typescript": "^4.9.4",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^6.0.0",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-runner-tsd": "^4.0.0",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "prisma": "^4.8.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.3",
    "ts-morph": "^17.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  }
}
