{
  "name": "toggl_dash_frontend",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently \"vite\" \"yarn codegen:watch\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check:lint": "eslint --fix --cache --ext .tsx,.ts,.js ./src",
    "check:format": "prettier . --write",
    "check:type": "tsc --project tsconfig.json --pretty --noEmit",
    "check:all": "npm-run-all -s check:type check:format check:lint",
    "pretest": "ts-node ./tools/storybook-test-utils/runner.ts",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "test:ss": "vitest run --config vite.config.snapshot.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch"
  },
  "lint-staged": {
    "*.{tsx,ts,js}": [
      "bash -c 'yarn check:type'",
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^2.9.9",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.8.3",
    "@tanstack/react-location": "^3.7.4",
    "@tanstack/react-table": "^8.7.9",
    "base-64": "^1.0.0",
    "cross-fetch": "^3.1.5",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.2",
    "jotai": "^2.0.3",
    "react": "^18.0.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^18.0.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.37.0",
    "ts-pattern": "^4.0.5",
    "uuid": "^9.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/client-preset": "^1.2.3",
    "@graphql-codegen/typescript": "^2.8.5",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@storybook/addon-actions": "^7.0.0-beta.19",
    "@storybook/addon-essentials": "^7.0.0-beta.19",
    "@storybook/addon-interactions": "^7.0.0-beta.19",
    "@storybook/addon-links": "^7.0.0-beta.19",
    "@storybook/addon-storyshots": "^7.0.0-beta.19",
    "@storybook/builder-vite": "^7.0.0-beta.19",
    "@storybook/jest": "^0.0.11-next.0",
    "@storybook/react": "^7.0.0-beta.19",
    "@storybook/react-vite": "^7.0.0-beta.19",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/testing-react": "^2.0.0-next.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/base-64": "^1.0.0",
    "@types/node": "^18.8.5",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.24.3",
    "@vitest/ui": "^0.29.7",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.6.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-storybook": "^0.6.6",
    "eslint-plugin-testing-library": "^5.5.1",
    "glob": "^9.3.2",
    "jest-extended": "^3.1.0",
    "jest-specific-snapshot": "^5.0.0",
    "jotai-devtools": "^0.3.1",
    "jsdom": "^20.0.1",
    "msw": "^0.49.2",
    "msw-storybook-addon": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "react-test-renderer": "^18.1.0",
    "storybook": "^7.0.0-beta.19",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
